<!-- Edit Modal -->
<div class="modal fade" id="editshiftmodal" data-bs-backdrop="static" tabindex="-1" aria-labelledby="exampleModalLabel"
  aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header" id="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Shift</h5>
        <i class='bx bx-x' data-bs-dismiss="modal" aria-label="CLose"></i>
      </div>
      <div class="modal-body" id="modal-body">
        <form>
          <div class="col">
            <input type="hidden" id="editshiftid" name="editshiftid">
            <div class="form-floating">
              <input type="text" class="form-control border-form-modal" id="editshift" name="editshift"
                value="8:00:00 - 17:00:00">
              <label for="floatingInput">Shift Hours</label>
            </div>
          </div>
          <br>
          <div class="col">
            <div class="form-floating">
              <select class="form-select border-form-modal" id="editshiftdepartment" name="editshiftdepartment"
                aria-label="Floating label select example">
                <option>Department</option>
              </select>
              <label for="floatingSelectGrid">Department</label>
            </div>
          </div>
          <br>
          <div class="col">
            <div class="form-floating">
              <select class="form-select border-form-modal" id="editshiftstatus" name="editshiftstatus"
                aria-label="Floating label select example">
                <option>Status</option>
                <option value="Active" selected>Active</option>
                <option value="Inactive">Inactive</option>
              </select>
              <label for="floatingSelectGrid">Shift</label>
            </div>
          </div>
          <br>
        </form>
      </div>
      <div class="modal-footer" id="modal-footer">
        <button type="button" class="btn btn-modal-close" data-bs-dismiss="modal">Close</button>
        <button type="button" class="btn btn-modal-save" id="editshiftsavebtn">Save</button>
      </div>
    </div>
  </div>
