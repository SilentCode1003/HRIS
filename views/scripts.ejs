<script>
     $(document).on("click", "#logoutBtn", function () {
          $.ajax({
            type: "POST",
            url: "/login/logout",
            success: function (result) {
                swal({
                    title: "success",
                    text: "Log in Successfully",
                    icon: "success",
                    button: "Lets gooo!!",
                  })
                  .then(results => {
                    window.location.reload();
                  location.replace("/");
                  });
                },
                error: function (err) {
                  errormsg(err);
                },
              });
            });
</script>

