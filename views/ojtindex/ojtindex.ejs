<div class="container">
  <div class="row">

    <!--- Map-->
    <div class="col-sm-8">
        <div class="card" id="card-map">
          <div class="card-header ch-map">
            <h6>
              <b>Map</b>
            </h6>
          </div>
          <div id="map"></div>
        </div>
      </div>

      <!---- Announcements ---->
    <div class="col-4">
      <div class="card" id="card-announcement">
        <div class="card-header ch-announcement">
          <h6>
            <b>Announcements</b>
          </h6>
        </div>
        <div id="carouselannouncement" class="carousel slide carousel-fade" data-bs-ride="carousel">
          <div class="carousel-inner">
          </div>
        </div>
      </div>
    </div>

  <!--- End of container & rows --->
  </div>
</div>


<div class="container">
  <div class="row">
       <!--- Clockin & Clockout ---->
       <div class="col-4 clock">
        <div class="card" id="clock-in-out">
          <lord-icon src="https://cdn.lordicon.com/lzgqzxrq.json" trigger="loop"
            colors="primary:#000000,secondary:#d1f3fa,tertiary:#66a1ee,quaternary:#f24c00" id="clock-lordicon">
          </lord-icon>
        <button type="button" class="btn btn-clockin" id="clockinbtn">
          Clock In
        </button>
        <button type="button" class="btn btn-clockout" id="clockoutbtn">
          Clock Out
        </button>
      </div>
    </div>
  
     <!---- Attendance ---->
     <div class="col-md-4">
      <div class="card" id="card-attendance">
        <div class="card-header ch-attendance">
          <h6>
            <b>Attendance</b>
          </h6>
        </div>
        <div class="card-body cb-attendance">
          <div class="table-responsive" id="table-attendance">
            <table class="table table-bordered" id="attendanceTable">
              <thead>
                <tr>
                  <th>Date</th>
                  <th>Type</th>
                  <th>Time</th>
                </tr>
              </thead>
            </table>
          </div>
        </div>
      </div>
    </div>

    <!---- Birthday ---->
    <div class="col-4">
      <div class="card" id="card-birthday">
        <div class="card-header ch-birthday">
          <h6>
            <b>Birthday</b>
          </h6>
        </div>
        <div aria-labelledby="nav-profile-tab">
          <div id="carouselbday" class="carousel slide carousel-fade" data-bs-ride="carousel">
            <div class="carousel-inner"></div>
          </div>
        </div>
      </div>
    </div>

   
<!--- End of container and rows-->
  </div>
</div>
