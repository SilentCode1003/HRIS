<!-- Table -->
<div class="card" id="tablecard">
    <div class="card-header ch-coop">
        <h3 class="title-card-table">Attendance</h3>
        <input id="datepicker" type="text" name="daterange" />
        <button id="checklogs" class="btn btn-add-color"><b>Check Logs</b></button>
        <button id="exportBtn" class="btn btn-add-color"><b>Export</b></button>
        <button id="loaddate" class="btn btn-add-color"><b>Load Date</b></button>
      <input id='myInput'  type='text' placeholder="  Search....">
    </div>
    <div class="card-body">
        <div class="table-responsive" id="table-responsive-id">
            <table class="table table-light table-bordered" id="dataTableattendance">
                <thead>
                        <!-- <th>Image</th> -->
                        <th class="col-sm-1">ID</th>
                        <th class="col-25">Name</th>
                        <th>Date</th>
                        <th class="col-sm-1">Time In</th>
                        <th class="col-sm-1">Time Out</th>
                        <th>Device In</th>
                        <th>Device Out</th>
                        <th class="col-sm-1">Total Hours</th>
                        <th class="col-sm-1">Action</th>
                    </tr>
                </thead>
                <tbody>
                </tbody>
            </table>
        </div>
    </div>
</div>
</div>
</div>

<!--MODAL-->

<div class="modal fade" id="editlogsmodal" tabindex="-1" role="dialog" aria-labelledby="totalCablingModal"
    aria-hidden="true">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header" id="modal-header">
                <div>
                    <h1 class="modal-title fs-5" id="exampleModalToggleLabel">
                       Logs
                    </h1>
                </div>
                <i class='bx bx-x' data-bs-dismiss="modal" aria-label="CLose"></i>
            </div>
            <div class="modal-body" id="modal-body">
                <div class="table-responsive" id="table-responsive-id">
                    <table class="table table-light table-striped" id="dataTableattendancelogs">
                        <thead>
                            <tr>
                                <th>Image</th>
                                <th class="col-25">Full Name</th>
                                <th>Date</th>
                                <th class="col-sm-1">Time</th>
                                <th>Log Type</th>
                                <th>Latitude</th>
                                <th>Lonitude</th>
                                <th class="col-sm-1">Device</th>
                                <th class="col-25">Location</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="modal-footer" id="modal-footer">
                <button type="button" class="btn btn-modal-close" data-bs-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
