<div class="container p-0">
    <div class="col-md-9 col-xl-12">
        <div class="tab-content">
            <div class="tab-pane fade show active" id="account" role="tabpanel">
                <div class="card settings-card">
                    <div class="card-header settings-header">
                        <h5 class="card-title mb-0">
                            <nav>
                                <div class="nav nav-tabs" id="nav-tab" role="tablist">
                                    <button class="nav-link settings-acc active" id="nav-home-tab" data-bs-toggle="tab"
                                        data-bs-target="#nav-home" type="button" role="tab" aria-controls="nav-home"
                                        aria-selected="true">Account</button>
                                    <button class="nav-link settings-pw" id="nav-profile-tab" data-bs-toggle="tab"
                                        data-bs-target="#nav-profile" type="button" role="tab"
                                        aria-controls="nav-profile" aria-selected="false">Password</button>
                            </nav>
                        </h5>
                    </div>
                    <div class="card-body settings-body">
                        <div class="tab-content" id="nav-tabContent">
                            <div class="tab-pane fade show active" id="nav-home" role="tabpanel"
                                aria-labelledby="nav-home-tab" tabindex="0">
                                <form>
                                    <div class="row">
                                        <div class="col-md-8">
                                            <div class="form-group">
                                                <label for="inputUsername" class="label-set">Username</label>
                                                <input type="text" class="form-control border-form-modal" id="inputUsername"
                                                    placeholder="Username" disabled>
                                            </div>
                                            <div class="form-row">
                                                <div class="form-group col-md-6">
                                                    <label for="inputFirstName" class="label-set">First name</label>
                                                    <input type="text" class="form-control border-form-modal" id="inputFirstName"
                                                        placeholder="First name" disabled>
                                                </div>
                                                <div class="form-group col-md-6">
                                                    <label for="inputLastName" class="label-set">Last name</label>
                                                    <input type="text" class="form-control border-form-modal" id="inputLastName"
                                                        placeholder="Last name" disabled>
                                                </div>
                                                <div class="form-group col-md-6">
                                                    <label for="inputcivilstatus" class="label-set">Civil Status</label>
                                                    <input type="text" class="form-control border-form-modal" id="inputcivilstatus"
                                                        placeholder="Civil Status" disabled>
                                                </div>
                                                <div class="form-group col-md-6">
                                                    <label for="inputjobstatus" class="label-set">Job Status</label>
                                                    <input type="text" class="form-control border-form-modal" id="inputjobstatus"
                                                        placeholder="Job Status" disabled>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label for="inputEmail4" class="label-set">Email</label>
                                                <input type="email" class="form-control border-form-modal" id="inputEmail4"
                                                    placeholder="Email" disabled>
                                            </div>
                                            <div class="form-group">
                                                <label for="inputAddress" class="label-set">Address</label>
                                                <input type="text" class="form-control border-form-modal" id="inputAddress"
                                                    placeholder="1234 Main St" disabled>
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="text-center">
                                                <img id="editpreview" style="width:200px; height:200px;"
                                                    class="rounded rounded-circle border-form-modal" alt="placeholder" disabled>
                                            </div>
                                            <br>
                                            <center>
                                                <div class="col">
                                                    <div class="col g-6" style="width: 200px;">
                                                    </div>
                                                </div>
                                            </center>
                                        </div>
                                    </div>
                                </form>
                            </div>
                            <div class="tab-pane fade" id="nav-profile" role="tabpanel"
                                aria-labelledby="nav-profile-tab" tabindex="0">
                                <form>
                                    <div class="form-group">
                                        <label for="inputPasswordCurrent" class="label-set">Current password</label>
                                        <div class="input-group">
                                            <input type="password" class="form-control border-form-modal" id="inputPasswordCurrent">
                                            <!-- <div class="input-group-append">
                                                <button class="btn btn-outline-secondary toggle-password" type="button">
                                                    <i class="fas fa-eye"></i>
                                                </button>
                                            </div> -->
                                        </div>
                                        <!-- <small><a href="#">Forgot your password?</a></small> -->
                                    </div>
                                    <div class="form-group">
                                        <label for="inputPasswordNew" class="label-set">New password</label>
                                        <div class="input-group">
                                            <input type="password" class="form-control border-form-modal" id="inputPasswordNew">
                                            <!-- <div class="input-group-append">
                                                <button class="btn btn-outline-secondary toggle-password" type="button">
                                                    <i class="fas fa-eye"></i>
                                                </button>
                                            </div> -->
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="inputPasswordNew2" class="label-set">Confirm password</label>
                                        <div class="input-group">
                                            <input type="password" class="form-control border-form-modal" id="inputPasswordNew2">
                                            <!-- <div class="input-group-append">
                                                <button class="btn btn-outline-secondary toggle-password" type="button">
                                                    <i class="fas fa-eye"></i>
                                                </button>
                                            </div> -->
                                        </div>
                                    </div>
                                    <button type="submit" id="saveBtn" class="btn btn-modal-save">Save Changes</button>
                                </form>
                            </div>
                            <div class="tab-pane fade" id="nav-contact" role="tabpanel"
                                aria-labelledby="nav-contact-tab" tabindex="0">...</div>
                            <div class="tab-pane fade" id="nav-disabled" role="tabpanel"
                                aria-labelledby="nav-disabled-tab" tabindex="0">...</div>
                        </div>
                    </div>
                </div>
                <br>
            </div>
        </div>
    </div>
</div>