<div class="card">
  <div class="card-content">
    <div class="card-header c-header px-4">
      <h3 class="title-card-table py-4">All Official Business Request</h3>
      <div class="row">
        <div class="col-md-6">
          <input
            id="search"
            type="text"
            placeholder="Search...."
            class="form-control w-100 my-2"
          />
        </div>
      </div>
    </div>

    <div class="card-body ">
      <div class="row mt-2">
        <div class="col">
          <div class="tableresponsive">
            <table class="table table-striped-light" id="datatable">
              <thead>
                <tr>
                  <th>ID</th>
                  <th>Employee ID</th>
                  <th>Employee Name</th>
                  <th>Attendance Date</th>
                  <th>Clock In</th>
                  <th>Clock Out</th>
                  <th>Applied Date</th>
                  <th>Status</th>
                </tr>
              </thead>
              <tbody></tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
