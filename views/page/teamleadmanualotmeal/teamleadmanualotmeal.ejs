<!--Add Modal-->
<div class="modal fade" id="addmodal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true" data-bs-backdrop="static">
  <div class="modal-dialog modal-xl">
    <div class="modal-content">
      <div class="modal-header" id="modal-header">
        <h3 class="modal-title fs-5" id="exampleModalLabel">Applied Overtime Meal?</h3>
        <i class='bx bx-x' data-bs-dismiss="modal" aria-label="CLose"></i>
      </div>
      <div class="modal-body" id="modal-body">
        <br>
        <div class="row">
          <div class="col-xl-2 col-lg-4" style="margin-left: 10px;">
            <div class="col g-6">
              <div class="p-4" style="width: 300px; margin-top: -100px;">
                <div class="text-center">
                  <img id="previewold" src="https://via.placeholder.com/250" class="border border-5 border-form-modal" alt="placeholder">
                </div>
                <br>
                <div class="col">
                  <div class="col g-6">
                    <input type="file" name="uploadtlmeal" id="uploadtlmeal" class="form-control border-form-modal" accept="image/*">
                  </div>
                </div>
                <h2 class="pb-3 mb-4 text-center modal-content-title">File Preview</h2>
              </div>
            </div>
          </div>
          <div class="col-xl-8 col-lg-7" style="margin-left: 150px;">
            <form>
              <br>
              <div class="row g-3">
                <div class="col">
                  <div class="form-floating">
                    <input type="date" class="form-control border-form-modal" id="addmealattendancedate" name="addmealattendancedate">
                    <label for="floatingInput">Attendance Date</label>
                  </div>
                </div>
                <div class="col">
                  <div class="form-floating">
                    <select type="text" class="form-control border-form-modal" id="addmealsubgroup" name="addmealsubgroup">
                      <option selected disabled>Choose Sub Group</option>
                    </select>
                    <label for="floatingInput">Sub Group Name</label>
                  </div>
                </div>
              </div>
              <br>
              <div class="row g-3">
                <div class="col">
                  <div class="form-floating clockin-manualot">
                    <input type="datetime-local" class="form-control border-form-modal" id="addmealclockin" name="addmealclockin"
                      placeholder="Address" readonly>
                    <label for="floatingInput">Clock In</label>
                  </div>
                </div>
                <div class="col">
                  <div class="form-floating clockout-manualot">
                    <input type="datetime-local" class="form-control border-form-modal" id="addmealclockout" name="addmealclockout"
                      placeholder="Address" readonly>
                    <label for="floatingInput">Clock Out</label>
                  </div>
                </div>
              </div>
              <br>
              <div class="row g-3">
                <div class="col">
                  <div class="form-floating">
                    <input type="text" class="form-control border-form-modal" id="addmealamount" name="addmealamount"
                      oninput="formatPriceTl(event, this)">
                    <label for="floatingInput">Meal Amount</label>
                  </div>
                </div>
                <div class="col">
                  <div class="form-floating">
                    <select class="form-select border-form-modal" id="addmealstatus" name="addmealstatus" aria-label="Floating label select example">
                      <option value="Applied">Applied</option>
                      <option value="Cancelled">Cancelled</option>
                    </select>
                    <label for="floatingInput">Status</label>
                  </div>
                </div>
              </div>
              <br>
              <div class="row g-3">
                <div class="col">
                  <div class="form-floating">
                    <select class="form-select border-form-modal" id="addmealpayrolldate" name="addmealpayrolldate"
                      aria-label="Floating label select example">
                    </select>
                    <label for="floatingInput">Payroll Date</label>
                  </div>
                </div>
              </div>
              <br>
              <div class="row g-3">
                <div class="col">
                  <div class="form-floating">
                    <textarea type="text" class="form-control border-form-modal" id="addmealreason" name="addmealreason"
                      placeholder="Type Your Reason"></textarea>
                    <label for="floatingInput">Reason</label>
                  </div>
                </div>
              </div>
              <br>
              <br>

            <div class="d-flex justify-content-end">
              <div class="flex-column d-flex gap-2">
                <h6 class="text-center authorised-approver">Authorized Approvers:</h6>
                <h6 class="text-center approver-teamlead" id="approverList"></h6>
                <!-- <label for="approverList">Authorized Approvers</label>
                                        <div class="form-floating approver-teamlead">
                                                      <ul id="approverList"></ul>
                                                    </div>
                                      </div> -->
              </div>
            </div>
            <br>
            <br>

            </form>
          </div>
        </div>
      </div>
      <div class="modal-footer" id="modal-footer">
        <button type="button" class="btn btn-modal-close" data-bs-dismiss="modal">Close</button>
        <button type="button" class="btn btn-modal-save" id="addmealsavebtn">Save</button>
      </div>
    </div>
  </div>
</div>

